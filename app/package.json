{
  "name": "intensity-plot",
  "description": "Electron + Create React App + Electron Builder",
  "version": "0.1.0",
  "private": true,
  "build": {
    "appId": "com.intensity.plot"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "build-electron": "electron-builder",
    "build-react": "react-scripts build",
    "build": "yarn build-electron",
    "prebuild": "yarn build-react",
    "test": "react-scripts test --env=jsdom",
    "start-electron": "electron --inspect=5858 .",
    "start-both": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron --inspect=5858 .\"",
    "start": "react-scripts start",
    "eject-react": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "concurrently": "^4.1.1",
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0",
    "fsevents": "^2.0.7",
    "react-scripts": "3.0.1",
    "wait-on": "^3.3.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cjs": "0.0.11",
    "cross-env": "^5.2.0",
    "electron-is-dev": "^1.1.0",
    "es6-shim": "^0.35.5",
    "jqplot": "^1.0.9",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "jstree": "^3.3.8",
    "popper.js": "^1.15.0",
    "rainbowvis.js": "^1.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "spin": "^0.0.1",
    "typescript": "^3.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
